* {
    primary: #bbc3ff;
    primary-fixed: #dfe0ff;
    primary-fixed-dim: #bbc3ff;
    on-primary: #242c61;
    on-primary-fixed: #0d154b;
    on-primary-fixed-variant: #3b4279;
    primary-container: #3b4279;
    on-primary-container: #dfe0ff;
    secondary: #c4c5dd;
    secondary-fixed: #e0e1f9;
    secondary-fixed-dim: #c4c5dd;
    on-secondary: #2d2f42;
    on-secondary-fixed: #181a2c;
    on-secondary-fixed-variant: #434559;
    secondary-container: #434559;
    on-secondary-container: #e0e1f9;
    tertiary: #e6bad7;
    tertiary-fixed: #ffd7f0;
    tertiary-fixed-dim: #e6bad7;
    on-tertiary: #45263d;
    on-tertiary-fixed: #2d1127;
    on-tertiary-fixed-variant: #5d3c54;
    tertiary-container: #5d3c54;
    on-tertiary-container: #ffd7f0;
    error: #ffb4ab;
    on-error: #690005;
    error-container: #93000a;
    on-error-container: #ffdad6;
    surface: #131318;
    on-surface: #e4e1e9;
    on-surface-variant: #c7c5d0;
    outline: #90909a;
    outline-variant: #46464f;
    shadow: #000000;
    scrim: #000000;
    inverse-surface: #e4e1e9;
    inverse-on-surface: #303036;
    inverse-primary: #525a92;
    surface-dim: #131318;
    surface-bright: #39393f;
    surface-container-lowest: #0d0e13;
    surface-container-low: #1b1b21;
    surface-container: #1f1f25;
    surface-container-high: #29292f;
    surface-container-highest: #34343a;
    background: #131318;
    on-background: #e4e1e9;
}

#window {
    background-color: var(surface);
    border: 0;
    padding: 2.5ch;
}

#mainbox {
    border: 0;
    padding: 0;
}

#message {
    border: 2px 0px 0px;
    border-color: var(surface-container);
    padding: 1px;
}

#textbox {
    text-color: var(on-surface);
}

#inputbar {
    children: [ prompt,textbox-prompt-colon,entry,case-indicator ];
}

#textbox-prompt-colon {
    expand: false;
    str: ":";
    margin: 0px 0.3em 0em 0em;
    text-color: var(on-surface);
}

#listview {
    fixed-height: 0;
    border: 2px 0px 0px;
    border-color: var(surface-container);
    spacing: 2px;
    scrollbar: true;
    padding: 2px 0px 0px;
}

#element {
    border: 0;
    padding: 1px;
}

#element-text, element-icon {
    background-color: inherit;
    text-color:       inherit;
}

#element.normal.normal {
    background-color: var(surface);
    text-color:       var(on-surface);
}

#element.normal.urgent {
    background-color: var(error);
    text-color:       var(on-error);
}

#element.normal.active {
    background-color: var(surface-container-high);
    text-color:       var(on-surface);
}

#element.selected.normal {
    background-color: var(primary-container);
    text-color:       var(on-primary-container);
}

#element.selected.urgent {
    background-color: var(error);
    text-color:       var(on-error);
}

#element.selected.active {
    background-color: var(secondary);
    text-color:       var(on-secondary);
}

#element.alternate.normal {
    background-color: var(surface-container-low);
    text-color:       var(on-surface);
}

#element.alternate.urgent {
    background-color: var(surface-container-low);
    text-color:       var(on-surface);
}

#element.alternate.active {
    background-color: var(surface-container-low);
    text-color:       var(on-surface);
}

#scrollbar {
    width: 0px;
    border: 0;
    handle-width: 0px;
    padding: 0;
}

#sidebar {
    border: 2px 0px 0px;
    border-color: var(surface-container);
}

#button {
    text-color: var(on-surface);
}

#button.selected {
    background-color: var(surface-container-high);
    text-color:       var(on-surface);
}

#inputbar {
    spacing: 0;
    text-color: var(on-surface);
    padding: 1px;
}

#case-indicator {
    spacing: 0;
    text-color: var(on-surface);
}

#entry {
    spacing: 0;
    text-color: var(on-surface);
}

#prompt {
    spacing: 0;
    text-color: var(on-surface);
}
